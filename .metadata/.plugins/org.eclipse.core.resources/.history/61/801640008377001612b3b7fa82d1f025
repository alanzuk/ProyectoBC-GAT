package general;

public  class Celda {

	protected int x;
	protected int y;
	
	
	public Celda(int x,int y){
		this.x=x;
		this.y=y;
	}
	
	public int getX(){
		return x;
	}
	
	public int getY(){
		return y;
	}
	
	public void moverX(int x){
		this.x+=x;
	}
	
	public void moverY(int y){
		this.y+=y;
	}
	
}
